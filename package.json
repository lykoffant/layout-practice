{
  "name": "layout-practice",
  "description": "Test task execution. Website layout.",
  "keywords": [
    "website",
    "ui-kit",
    "layout",
    "webpack",
    "pug",
    "scss",
    "javascript"
  ],
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "npm run build && git add dist/ && git commit",
    "format": "prettier --write './src/**/*.(pug|scss|js)'",
    "check": "npm-run-all check:*",
    "check:prettier": "prettier --check './src/**/*.(pug|scss|js)'",
    "check:stylelint": "stylelint './src/**/*.(scss|css)'",
    "check:eslint": "eslint './src/**/*.js'"
  },
  "author": {
    "name": "Anton Lykov",
    "email": "lykoffant@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lykoffant/layout-practice.git"
  },
  "bugs": {
    "url": "https://github.com/lykoffant/layout-practice/issues"
  },
  "homepage": "https://github.com/lykoffant/layout-practice#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@prettier/plugin-pug": "^1.13.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.22.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-webpack-plugin": "^2.5.2",
    "favicons": "^6.2.1",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-run-all": "^4.1.5",
    "obsolete-webpack-plugin": "^0.5.6",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
